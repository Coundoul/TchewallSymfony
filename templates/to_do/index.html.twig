{% extends 'template.base.twig' %}


{% block Title %}
    To-DO
{% endblock %}



{% block body %}

{% block PageTitle %} Liste des ToDo : {% endblock %}
    <ol class="list-group">
        {% for cle, valeur in app.session.get('todos') %}
            <li class="list-group-item">
                {{ cle }} : {{ valeur }}
            </li>
        {% endfor %}
    </ol>
{% endblock %}